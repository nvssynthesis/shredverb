@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix bufs:  <http://lv2plug.in/ns/ext/buf-size#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix opts:  <http://lv2plug.in/ns/ext/options#> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix pg:    <http://lv2plug.in/ns/ext/port-groups#> .
@prefix plug:  <https://nvssynthesis.github.io//plugins/shredVerb:> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rsz:   <http://lv2plug.in/ns/ext/resize-port#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .
@prefix time:  <http://lv2plug.in/ns/ext/time#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

plug:feedbackGain
	a lv2:Parameter ;
	rdfs:label "Feedback Gain" ;
	rdfs:range atom:Float ;
	lv2:default 0.1 ;
	lv2:minimum 0 ;
	lv2:maximum 0.7071 .

plug:size
	a lv2:Parameter ;
	rdfs:label "Spatial Size" ;
	rdfs:range atom:Float ;
	lv2:default 0.2 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:lop
	a lv2:Parameter ;
	rdfs:label "LP Freq" ;
	rdfs:range atom:Float ;
	lv2:default 12000 ;
	lv2:minimum 200 ;
	lv2:maximum 20000 .

plug:dryWet
	a lv2:Parameter ;
	rdfs:label "Dry/Wet" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:tvap_0
	a lv2:Parameter ;
	rdfs:label "TVAP 0" ;
	rdfs:range atom:Float ;
	lv2:default 200 ;
	lv2:minimum 20 ;
	lv2:maximum 20000 .

plug:tvap_1
	a lv2:Parameter ;
	rdfs:label "TVAP 1" ;
	rdfs:range atom:Float ;
	lv2:default 2000 ;
	lv2:minimum 20 ;
	lv2:maximum 20000 .

plug:tvap_2
	a lv2:Parameter ;
	rdfs:label "TVAP 2" ;
	rdfs:range atom:Float ;
	lv2:default 500 ;
	lv2:minimum 20 ;
	lv2:maximum 20000 .

plug:tvap_3
	a lv2:Parameter ;
	rdfs:label "TVAP 3" ;
	rdfs:range atom:Float ;
	lv2:default 9000 ;
	lv2:minimum 20 ;
	lv2:maximum 20000 .

plug:dist1iner
	a lv2:Parameter ;
	rdfs:label "Distortion 1 Inner" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:dist1outr
	a lv2:Parameter ;
	rdfs:label "Distortion 1 Outer" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:dist2iner
	a lv2:Parameter ;
	rdfs:label "Distortion 2 Inner" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:dist2outr
	a lv2:Parameter ;
	rdfs:label "Distortion 2 Outer" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:interp
	a lv2:Parameter ;
	rdfs:label "Interpolation Type" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:randomizzze
	a lv2:Parameter ;
	rdfs:label "Randomize" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:input_group_1
	a pg:InputGroup ;
	lv2:name "Input" ;
	lv2:symbol "input_group_1" .

plug:output_group_1
	a pg:OutputGroup ;
	lv2:name "Output" ;
	lv2:symbol "output_group_1" .

<https://nvssynthesis.github.io//plugins/shredVerb>
	ui:ui <https://nvssynthesis.github.io//plugins/shredVerb:UI> ;
	a lv2:Plugin ;
	doap:name "shredVerb" ;
	doap:description "effed up reverb" ;
	lv2:minorVersion 0 ;
	lv2:microVersion 0 ;
	doap:maintainer [
		a foaf:Person ;
		foaf:name "nvssynthesis" ;
		foaf:homepage <https://nvssynthesis.github.io/> ;
		foaf:mbox <nicholas93solem@gmail.com> ;
	] ;
	doap:release [
		a doap:Version ;
		doap:revision "1.0.0" ;
	] ;
	lv2:optionalFeature
		lv2:hardRTCapable ;
	lv2:extensionData
		state:interface ;
	lv2:requiredFeature
		urid:map ,
		opts:options ,
		bufs:boundedBlockLength ;
	pg:mainInput plug:input_group_1 ;
	pg:mainOutput plug:output_group_1 ;
	patch:writable
		plug:feedbackGain ,
		plug:size ,
		plug:lop ,
		plug:dryWet ,
		plug:tvap_0 ,
		plug:tvap_1 ,
		plug:tvap_2 ,
		plug:tvap_3 ,
		plug:dist1iner ,
		plug:dist1outr ,
		plug:dist2iner ,
		plug:dist2outr ,
		plug:interp ,
		plug:randomizzze ;
	patch:readable
		plug:feedbackGain ,
		plug:size ,
		plug:lop ,
		plug:dryWet ,
		plug:tvap_0 ,
		plug:tvap_1 ,
		plug:tvap_2 ,
		plug:tvap_3 ,
		plug:dist1iner ,
		plug:dist1outr ,
		plug:dist2iner ,
		plug:dist2outr ,
		plug:interp ,
		plug:randomizzze ;
	lv2:port [
		a lv2:InputPort , lv2:AudioPort ;
		lv2:index 0 ;
		lv2:symbol "audio_in_1" ;
		lv2:name "Audio In 1" ;
		pg:group plug:input_group_1 ;
		lv2:designation <http://lv2plug.in/ns/ext/port-groups#left> ;
	] , [
		a lv2:InputPort , lv2:AudioPort ;
		lv2:index 1 ;
		lv2:symbol "audio_in_2" ;
		lv2:name "Audio In 2" ;
		pg:group plug:input_group_1 ;
		lv2:designation <http://lv2plug.in/ns/ext/port-groups#right> ;
	] , [
		a lv2:OutputPort , lv2:AudioPort ;
		lv2:index 2 ;
		lv2:symbol "audio_out_1" ;
		lv2:name "Audio Out 1" ;
		pg:group plug:output_group_1 ;
		lv2:designation <http://lv2plug.in/ns/ext/port-groups#left> ;
	] , [
		a lv2:OutputPort , lv2:AudioPort ;
		lv2:index 3 ;
		lv2:symbol "audio_out_2" ;
		lv2:name "Audio Out 2" ;
		pg:group plug:output_group_1 ;
		lv2:designation <http://lv2plug.in/ns/ext/port-groups#right> ;
	] , [
		a lv2:InputPort , atom:AtomPort ;
		rsz:minimumSize 9200 ;
		atom:bufferType atom:Sequence ;
		atom:supports
			patch:Message ,
			time:Position ;
		lv2:designation lv2:control ;
		lv2:index 4 ;
		lv2:symbol "in" ;
		lv2:name "In" ;
	] , [
		a lv2:OutputPort , atom:AtomPort ;
		rsz:minimumSize 9200 ;
		atom:bufferType atom:Sequence ;
		atom:supports
			patch:Message ;
		lv2:designation lv2:control ;
		lv2:index 5 ;
		lv2:symbol "out" ;
		lv2:name "Out" ;
	] , [
		a lv2:OutputPort , lv2:ControlPort ;
		lv2:designation lv2:latency ;
		lv2:symbol "latency" ;
		lv2:name "Latency" ;
		lv2:index 6 ;
		lv2:portProperty lv2:reportsLatency , lv2:integer , lv2:connectionOptional , pprop:notOnGUI ;
		units:unit units:frame ;
	] , [
		a lv2:InputPort , lv2:ControlPort ;
		lv2:designation lv2:freeWheeling ;
		lv2:symbol "freeWheeling" ;
		lv2:name "Free Wheeling" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:index 7 ;
		lv2:portProperty lv2:toggled , lv2:connectionOptional , pprop:notOnGUI ;
	] , [
		a lv2:InputPort , lv2:ControlPort ;
		lv2:designation lv2:enabled ;
		lv2:symbol "enabled" ;
		lv2:name "Enabled" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:index 8 ;
		lv2:portProperty lv2:toggled , lv2:connectionOptional , pprop:notOnGUI ;
	] ;
	opts:supportedOption
		bufs:maxBlockLength .
